    "eslint": "^9.29.0",
    "eslint-formatter-gitlab": "^6.0.1",
    "markdown-toc": "^1.2.0",
    "remark-cli": "^12.0.1",
    "remark-lint": "^10.0.1",
    "remark-preset-lint-recommended": "^7.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.1"
  },
  "scripts": {
    "lint-md": "remark ."
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
