{
  "name": "nabla-hooks",
  "description": "Hooks sample",
  "branchVersion": "^1.0.6",
  "branchPattern": "1.0.*",
  "version": "1.0.6",
  "pre-commit": [
    "precommit"
  ],
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.8.2",
    "node-gyp": ">=3.0.3"
  },
  "engine-strict": true,
  "browserslist": [
    "last 1 version",
    "> 1%",
