{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "master"
	},
	"files": { "ignoreUnknown": true },
	"linter": { "enabled": true, "rules": { "recommended": false } },
	"json": { "parser": { "allowComments": true, "allowTrailingCommas": true } }
}
